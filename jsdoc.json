{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "opts": {
    "template": "node_modules/docdash",
    "destination": "./docs"
  },
  "source": {
    "include": [
      "./lib/privileges.js",
      "./lib/storages/memory.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}